{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "printWidth": 100,
  "parser": "babel",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "all",
  "useTabs": false,
  "overrides": [
    {
      "files": ["**/*.json", ".*rc"],
      "options": {
        "parser": "json"
      }
    },
    {
      "files": ["**/*.css"],
      "options": {
        "parser": "css"
      }
    }
  ]
}
